{
    "dataset": {
        "extract_voice": false,
        "in_memory_percentage": 1,
        "noise_only_percent": 0.0,
        "num_condition_classes": 0,
        "path": "data/NSDTSEA/",
        "regain": 0.06,
        "sample_rate": 8000,
        "type": "wsj0-mix"
    },
    "model": {
        "condition_encoding": "binary",
        "dilations": 9,
        "filters": {
            "lengths": {
                "res": 3,
                "final": [3, 3],
                "skip": 1
            },
            "depths": {
                "res": 128,
                "skip": 128,
                "final": [2048, 256]
            }
        },
        "num_stacks": 3,
        "target_field_length": 1601,
        "target_padding": 1
    },
    "optimizer": {
        "decay": 0.0,
        "epsilon": 1e-08,
        "lr": 0.001,
        "momentum": 0.9,
        "type": "adam"
    },
    "training": {
        "batch_size": 32,
        "early_stopping_patience": 16,
        "loss": {
            "out_1": {
                "l1": 1,
                "l2": 0,
                "weight": 1
            },
            "out_2": {
                "l1": 1,
                "l2": 0,
                "weight": 1
            }
        },
        "num_epochs": 150,
        "num_test_samples" : 3000,
        "num_valid_samples": 5000,
        "num_train_samples": 20000,
        "path": "sessions/002",
        "verbosity": 1
    }
}
